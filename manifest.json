{
	"manifest_version": 2,
	"name": "YouTube Incognito Theatre mode",
	"version": "1.2.1",
	"description": "Always disable autoplay and use theatre mode, even when browsing incognito.",
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"64": "icon64.png",
		"128": "icon128.png"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"minimum_chrome_version": "12.0.707.0",
	"incognito": "split",
	"permissions": [
		"https://www.youtube.com/",
		"https://vimeo.com/",
		"cookies"
	]
}
